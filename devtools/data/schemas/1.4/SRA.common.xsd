                                                <ann:Status current="Active"/>
                                            </xsd:appinfo>
                                        </xsd:annotation>
                                    </xsd:enumeration>
                                    <xsd:enumeration value="Illumina Genome Analyzer IIx">
                                        <xsd:annotation>
                                            <xsd:documentation>Illumina Genome Analyzer IIx system</xsd:documentation>
                                            <xsd:appinfo>
                                                <ann:Glossary/>
                                                <ann:Status current="Active"/>
                                            </xsd:appinfo>
                                        </xsd:annotation>
                                    </xsd:enumeration>
                                    <xsd:enumeration value="Illumina HiSeq 2500">
                                        <xsd:annotation>
                                            <xsd:documentation>Illumina HiSeq 2500 system</xsd:documentation>
                                            <xsd:appinfo>
                                                <ann:Glossary/>
                                                <ann:Status current="Active"/>
                                            </xsd:appinfo>
                                        </xsd:annotation>
                                    </xsd:enumeration>
                                    <xsd:enumeration value="Illumina HiSeq 2000">
                                        <xsd:annotation>
                                            <xsd:documentation>Illumina HiSeq 2000 system</xsd:documentation>
                                            <xsd:appinfo>
                                                <ann:Glossary/>
                                                <ann:Status current="Active"/>
                                            </xsd:appinfo>
                                        </xsd:annotation>
                                    </xsd:enumeration>
                                    <xsd:enumeration value="Illumina HiSeq 1000">
                                        <xsd:annotation>
                                            <xsd:documentation>Illumina HiSeq 1000 system</xsd:documentation>
                                            <xsd:appinfo>
                                                <ann:Glossary/>
                                                <ann:Status current="Active"/>
                                            </xsd:appinfo>
                                        </xsd:annotation>
                                    </xsd:enumeration>
                                    <xsd:enumeration value="Illumina MiSeq">
                                        <xsd:annotation>
                                            <xsd:documentation>Illumina MiSeq system</xsd:documentation>
                                            <xsd:appinfo>
                                                <ann:Glossary/>
                                                <ann:Status current="Active"/>
                                            </xsd:appinfo>
                                        </xsd:annotation>
