                                                <ann:Status current="Deprecated"/>
                                                <ann:Alternative xpath="/PLATFORM/ABI_SOLID/INSTRUMENT_MODEL/AB 5500xl Genetic Analyzer"/>
                                            </xsd:appinfo>
                                        </xsd:annotation>
                                    </xsd:enumeration>
                                    <xsd:enumeration value="AB 5500 Genetic Analyzer">
                                        <xsd:annotation>
                                            <xsd:documentation>AB 5500 Genetic Analyzer</xsd:documentation>
                                            <xsd:appinfo>
                                                <ann:Glossary/>
                                                <ann:Status current="Active"/>
                                            </xsd:appinfo>
                                        </xsd:annotation>
                                    </xsd:enumeration>
                                    <xsd:enumeration value="AB 5500xl Genetic Analyzer">
                                        <xsd:annotation>
                                            <xsd:documentation>AB 5500xl Genetic Analyzer</xsd:documentation>
                                            <xsd:appinfo>
                                                <ann:Glossary/>
                                                <ann:Status current="Active"/>
                                            </xsd:appinfo>
                                        </xsd:annotation>
                                    </xsd:enumeration>
                                    <xsd:enumeration value="unspecified"/>
                                </xsd:restriction>
                            </xsd:simpleType>
                        </xsd:element>
                        <xsd:element name="COLOR_MATRIX" minOccurs="0" maxOccurs="1">
                            <xsd:annotation>
                                <xsd:documentation> DEPRECATED. </xsd:documentation>
                                <xsd:appinfo>
                                    <ann:Glossary/>
                                    <ann:Status current="Deprecated"/>
                                </xsd:appinfo>
                            </xsd:annotation>
                            <xsd:complexType>
                                <xsd:sequence>
                                    <xsd:element name="COLOR" minOccurs="1" maxOccurs="unbounded">
                                        <xsd:complexType mixed="true">
                                            <xsd:simpleContent>
                                                <xsd:extension base="xs:string">
                                                    <xsd:attribute name="dibase" type="xs:string"/>
