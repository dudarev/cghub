<?xml version="1.0" encoding="UTF-8"?>
<!-- INSDC Sequence Read Archive resource Common object XML specification -->
<!-- $Id$ -->
<xsd:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:ann="SRA.annotation"
    xmlns:com="SRA.common" targetNamespace="SRA.common">
    <!-- <xsd:import schemaLocation="http://www.ncbi.nlm.nih.gov/viewvc/v1/trunk/sra/doc/SRA_1-4/SRA.annotation.xsd?view=co"
        namespace="SRA.annotation"/> -->
    <xsd:import schemaLocation="SRA.annotation.xsd" namespace="SRA.annotation"/>

    <xsd:attributeGroup name="NameGroup">
        <xsd:attribute name="alias" type="xs:string" use="optional">
            <xsd:annotation>
                <xsd:documentation> Submitter designated name of the SRA document of this type. At minimum alias should be unique
                    throughout the submission of this document type. If center_name is specified, the name should be unique in all
                    submissions from that center of this document type. </xsd:documentation>
                <xsd:appinfo>
                    <ann:Glossary/>
                    <ann:Status current="Active"/>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="center_name" type="xs:string" use="optional">
            <xsd:annotation>
                <xsd:documentation> Owner authority of this document and namespace for submitter's name of this document. If not
                    provided, then the submitter is regarded as "Individual" and document resolution can only happen within the
                    submission. </xsd:documentation>

                <xsd:appinfo>
                    <ann:Glossary/>
                    <ann:Status current="Active"/>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="broker_name" type="xs:string" use="optional">
            <xsd:annotation>
                <xsd:documentation> Broker authority of this document. If not provided, then the broker is considered "direct". </xsd:documentation>
                <xsd:appinfo>
                    <ann:Glossary/>
                    <ann:Status current="Active"/>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="accession" type="xs:string" use="optional">
            <xsd:annotation>
                <xsd:documentation> The document's accession as assigned by the Home Archive. </xsd:documentation>
                <xsd:appinfo>
                    <ann:Glossary/>
                    <ann:Status current="Active"/>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
    </xsd:attributeGroup>

    <xsd:attributeGroup name="RefNameGroup">
        <xsd:attribute name="refname" type="xs:string" use="optional">
            <xsd:annotation>
                <xsd:documentation> Identifies a record by name that is known within the namespace defined by attribute "refcenter"
                    Use this field when referencing an object for which an accession has not yet been issued. </xsd:documentation>
                <xsd:appinfo>
                    <ann:Glossary/>
                    <ann:Status current="Active"/>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="refcenter" type="xs:string" use="optional">
            <xsd:annotation>
                <xsd:documentation> The center namespace of the attribute "refname". When absent, the namespace is assumed to be the
                    current submission. </xsd:documentation>
                <xsd:appinfo>
                    <ann:Glossary/>
                    <ann:Status current="Active"/>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="accession" type="xs:string" use="optional">
            <xsd:annotation>
                <xsd:documentation> Identifies a record by its accession. The scope of resolution is the entire Archive. </xsd:documentation>
                <xsd:appinfo>
                    <ann:Glossary/>
                    <ann:Status current="Active"/>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
    </xsd:attributeGroup>

    <xsd:complexType name="IdentifierNodeType">
        <xsd:annotation>
            <xsd:documentation>Abstract type for node in IDENTIFIER block.</xsd:documentation>
            <xsd:appinfo>
                <ann:Glossary/>
                <ann:Status current="Active"/>
            </xsd:appinfo>
        </xsd:annotation>
        <xsd:simpleContent>
            <xsd:extension base="xs:string">
                <xsd:attribute name="label" use="optional" type="xs:string">
                    <xsd:annotation>
                        <xsd:documentation>A string value that can be used as a display hint, or to qualify a non-SRA
                            identifier.</xsd:documentation>
                        <xsd:appinfo>
                            <ann:Glossary/>
                            <ann:Status current="Active"/>
                        </xsd:appinfo>
                    </xsd:annotation>
                </xsd:attribute>
            </xsd:extension>
        </xsd:simpleContent>
    </xsd:complexType>

    <xsd:complexType name="AccessionType">
        <xsd:simpleContent>
            <xsd:extension base="com:IdentifierNodeType"> </xsd:extension>
        </xsd:simpleContent>
    </xsd:complexType>

    <xsd:complexType name="NameType">
        <xsd:simpleContent>
            <xsd:extension base="com:IdentifierNodeType">
                <xsd:attribute name="namespace" use="required" type="xs:string">
                    <xsd:annotation>
                        <xsd:documentation>A string value that constrains the domain of named identifiers (namespace). </xsd:documentation>
                        <xsd:appinfo>
                            <ann:Glossary/>
                            <ann:Status current="Active"/>
                        </xsd:appinfo>
                    </xsd:annotation>
                </xsd:attribute>
            </xsd:extension>
        </xsd:simpleContent>

    </xsd:complexType>

    <xsd:complexType name="UUIDType">
        <xsd:simpleContent>
            <xsd:extension base="com:IdentifierNodeType"> </xsd:extension>
        </xsd:simpleContent>
    </xsd:complexType>

    <xsd:complexType name="IdentifierType">
        <xsd:annotation>
            <xsd:documentation>Set of identifiers that can dereference the record. This element can specify a set of records each
                referenced by IdentifierType that can be the successor (replaces), the replacement set (split), that can be the
                replacer set (join), or can be the synonym (secondary accession). </xsd:documentation>
            <xsd:appinfo>
                <ann:Status current="Active"/>
            </xsd:appinfo>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element name="PRIMARY_ID" type="com:AccessionType" minOccurs="0" maxOccurs="1">
                <xsd:annotation>
                    <xsd:documentation>A primary key in an INSDC primary data database.</xsd:documentation>
                    <xsd:appinfo>
                        <ann:Glossary/>
                        <ann:Status current="Active"/>
                        <ann:BusinessRules>
                            <Rule>Not used on submission.</Rule>
                        </ann:BusinessRules>
                    </xsd:appinfo>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="SECONDARY_ID" type="com:AccessionType" minOccurs="0" maxOccurs="unbounded">
                <xsd:annotation>
                    <xsd:documentation>A secondary or defunct primary key in an INSDC primary data database.</xsd:documentation>
                    <xsd:appinfo>
                        <ann:Glossary/>
                        <ann:Status current="Active"/>
                    </xsd:appinfo>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="EXTERNAL_ID" type="com:NameType" minOccurs="0" maxOccurs="unbounded">
                <xsd:annotation>
                    <xsd:documentation>An identifier from another non-INSDC database qualified by a namespace.</xsd:documentation>
                    <xsd:appinfo>
                        <ann:Glossary/>
                        <ann:Status current="Active"/>
                    </xsd:appinfo>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="SUBMITTER_ID" type="com:NameType" minOccurs="0" maxOccurs="unbounded">
                <xsd:annotation>
                    <xsd:documentation>A local identifier provided by a submitter and qualified by a namespace.</xsd:documentation>
                    <xsd:appinfo>
                        <ann:Glossary/>
                        <ann:Status current="Active"/>
                    </xsd:appinfo>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="UUID" type="com:UUIDType" minOccurs="0" maxOccurs="1">
                <xsd:annotation>
                    <xsd:documentation>A universally unique identifier that requires no namespace.</xsd:documentation>
                    <xsd:appinfo>
                        <ann:Glossary/>
                        <ann:Status current="Active"/>
                        <ann:Scope>non-INSDC</ann:Scope>
                        <ann:DocumentationLinks>
                            <com:URL_LINK>
                                <com:URL>http://en.wikipedia.org/wiki/Universally_unique_identifier</com:URL>
                            </com:URL_LINK>
                        </ann:DocumentationLinks>
                    </xsd:appinfo>
                </xsd:annotation>
            </xsd:element>
        </xsd:sequence>
    </xsd:complexType>

    <xsd:complexType name="XRefType">
        <xsd:all>
            <xsd:element name="DB" type="xs:string" minOccurs="1" maxOccurs="1">
                <xsd:annotation>
                    <xsd:documentation> INSDC controlled vocabulary of permitted cross references. Please see
                        http://www.insdc.org/db_xref.html . For example, FLYBASE. </xsd:documentation>
                    <xsd:appinfo>
                        <ann:Glossary/>
                        <ann:Status current="Active"/>
                    </xsd:appinfo>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="ID" minOccurs="1" maxOccurs="1" type="xs:string">
                <xsd:annotation>
                    <xsd:documentation> Accession in the referenced database. For example, FBtr0080008 (in FLYBASE). </xsd:documentation>
                    <xsd:appinfo>
                        <ann:Glossary/>
                        <ann:Status current="Active"/>
                    </xsd:appinfo>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="LABEL" type="xs:string" minOccurs="0" maxOccurs="1">
                <xsd:annotation>
                    <xsd:documentation> Text label to display for the link. </xsd:documentation>
                    <xsd:appinfo>
                        <ann:Glossary/>
                        <ann:Status current="Active"/>
                    </xsd:appinfo>
                </xsd:annotation>
            </xsd:element>
        </xsd:all>
    </xsd:complexType>
    <xsd:complexType name="AttributeType">
        <xsd:annotation>
            <xsd:documentation> Reusable attributes to encode tag-value pairs with optional units. </xsd:documentation>
            <xsd:appinfo>
                <ann:Glossary/>
                <ann:Status current="Active"/>
            </xsd:appinfo>
        </xsd:annotation>
        <xsd:all>
            <xsd:element name="TAG" type="xs:string" minOccurs="1" maxOccurs="1">
                <xsd:annotation>
                    <xsd:documentation> Name of the attribute. </xsd:documentation>
                    <xsd:appinfo>
                        <ann:Glossary/>
                        <ann:Status current="Active"/>
                    </xsd:appinfo>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="VALUE" type="xs:string" minOccurs="1" maxOccurs="1">
                <xsd:annotation>
                    <xsd:documentation> Value of the attribute. </xsd:documentation>
                    <xsd:appinfo>
                        <ann:Glossary/>
                        <ann:Status current="Active"/>
                    </xsd:appinfo>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="UNITS" type="xs:string" minOccurs="0" maxOccurs="1">
                <xsd:annotation>
                    <xsd:documentation> Optional scientific units. </xsd:documentation>
                    <xsd:appinfo>
                        <ann:Glossary/>
                        <ann:Status current="Active"/>
                    </xsd:appinfo>
                </xsd:annotation>
            </xsd:element>
        </xsd:all>
    </xsd:complexType>
    <xsd:complexType name="SraLinkType">
        <xsd:annotation>
            <xsd:documentation> The SraLinkType mechanism encodes local references between SRA objects. These references are local
                to the Home Archive and during archival are resolved to exportable references suitable for mirroring between
                Archives. SraLinks can be used as temporary place holders for pre-published or post-suppressed relationships that
                will not be mirrored between Archives. </xsd:documentation>
            <xsd:appinfo>
                <ann:Glossary/>
                <ann:Status current="Active"/>
            </xsd:appinfo>
        </xsd:annotation>
        <xsd:attribute name="sra_object_type" use="optional">
            <xsd:annotation>
                <xsd:documentation> SRA link type. </xsd:documentation>
                <xsd:appinfo>
                    <ann:Glossary/>
                    <ann:Status current="Active"/>
                </xsd:appinfo>
            </xsd:annotation>
            <xsd:simpleType>
                <xsd:restriction base="xs:string">
                    <xsd:enumeration value="STUDY"/>
                    <xsd:enumeration value="SAMPLE"/>
                    <xsd:enumeration value="ANALYSIS"/>
                    <xsd:enumeration value="EXPERIMENT"/>
                    <xsd:enumeration value="RUN"/>
                </xsd:restriction>
            </xsd:simpleType>
        </xsd:attribute>
        <xsd:attributeGroup ref="com:RefNameGroup"/>
    </xsd:complexType>
    <xsd:complexType name="LinkType">
        <xsd:annotation>
            <xsd:documentation> Reusable external links type to encode URL links, Entrez links, and db_xref links. </xsd:documentation>
            <xsd:appinfo>
                <ann:Glossary/>
                <ann:Status current="Active"/>
            </xsd:appinfo>
        </xsd:annotation>
        <xsd:choice>
            <xsd:element name="SRA_LINK" type="com:SraLinkType"/>
            <xsd:element name="URL_LINK">
                <xsd:complexType>
                    <xsd:all>
                        <xsd:element name="LABEL" type="xs:string" minOccurs="1" maxOccurs="1">
                            <xsd:annotation>
                                <xsd:documentation> Text label to display for the link. </xsd:documentation>
                                <xsd:appinfo>
                                    <ann:Glossary/>
                                    <ann:Status current="Active"/>
                                </xsd:appinfo>
                            </xsd:annotation>
                        </xsd:element>
                        <xsd:element name="URL" minOccurs="1" maxOccurs="1" type="xsd:anyURI">
                            <xsd:annotation>
                                <xsd:documentation> The internet service link (file:, http:, ftp:, etc). </xsd:documentation>
                                <xsd:appinfo>
                                    <ann:Glossary/>
                                    <ann:Status current="Active"/>
                                </xsd:appinfo>
                            </xsd:annotation>
                        </xsd:element>
                    </xsd:all>
                </xsd:complexType>
            </xsd:element>
            <xsd:element name="XREF_LINK" type="com:XRefType"/>

            <xsd:element name="ENTREZ_LINK">
                <xsd:complexType>
                    <xsd:sequence>
                        <xsd:element name="DB" type="xs:string" minOccurs="1" maxOccurs="1">
                            <xsd:annotation>
                                <xsd:documentation> NCBI controlled vocabulary of permitted cross references. Please see
                                    http://www.ncbi.nlm.nih.gov/entrez/eutils/einfo.fcgi? . </xsd:documentation>
                                <xsd:appinfo>
                                    <ann:Glossary/>
                                    <ann:Status current="Active"/>
                                </xsd:appinfo>
                            </xsd:annotation>
                        </xsd:element>
                        <xsd:choice>
                            <xsd:element name="ID" type="xsd:nonNegativeInteger" minOccurs="1" maxOccurs="1">
                                <xsd:annotation>
                                    <xsd:documentation> Numeric record id meaningful to the NCBI Entrez system. </xsd:documentation>
                                    <xsd:appinfo>
                                        <ann:Glossary/>
                                        <ann:Status current="Active"/>
                                    </xsd:appinfo>
                                </xsd:annotation>
                            </xsd:element>
                            <xsd:element name="QUERY" type="xs:string" minOccurs="1" maxOccurs="1">
                                <xsd:annotation>
                                    <xsd:documentation> Accession string meaningful to the NCBI Entrez system. </xsd:documentation>
                                    <xsd:appinfo>
                                        <ann:Glossary/>
                                        <ann:Status current="Active"/>
                                    </xsd:appinfo>
                                </xsd:annotation>
                            </xsd:element>
                        </xsd:choice>
                        <xsd:element name="LABEL" type="xs:string" minOccurs="0" maxOccurs="1">
                            <xsd:annotation>
                                <xsd:documentation> How to label the link. </xsd:documentation>
                                <xsd:appinfo>
                                    <ann:Glossary/>
                                    <ann:Status current="Active"/>
                                </xsd:appinfo>
                            </xsd:annotation>
                        </xsd:element>
                    </xsd:sequence>
                </xsd:complexType>
            </xsd:element>
            <xsd:element name="DDBJ_LINK">
                <xsd:complexType>
                    <xsd:sequence>
                        <xsd:element name="DB" type="xs:string" minOccurs="1" maxOccurs="1">
                            <xsd:annotation>
                                <xsd:documentation>DDBJ controlled vocabulary of permitted cross references. </xsd:documentation>
                                <xsd:appinfo>
                                    <ann:Glossary/>
                                    <ann:Status current="Active"/>
                                </xsd:appinfo>
                            </xsd:annotation>
                        </xsd:element>
                        <xsd:element name="ID" minOccurs="1" maxOccurs="1" type="xs:string">
                            <xsd:annotation>
                                <xsd:documentation>Accession in the referenced database. </xsd:documentation>
                                <xsd:appinfo>
                                    <ann:Glossary/>
                                    <ann:Status current="Active"/>
                                </xsd:appinfo>
                            </xsd:annotation>
                        </xsd:element>
                        <xsd:element name="URL" minOccurs="0" maxOccurs="1" type="xsd:anyURI">
                            <xsd:annotation>
                                <xsd:documentation>An URL to the cross-references accession. </xsd:documentation>
                                <xsd:appinfo>
                                    <ann:Glossary/>
                                    <ann:Status current="Active"/>
                                </xsd:appinfo>
                            </xsd:annotation>
                        </xsd:element>
                        <xsd:element name="LABEL" type="xs:string" minOccurs="0" maxOccurs="1">
                            <xsd:annotation>
                                <xsd:documentation>A textual description of the cross-reference. </xsd:documentation>
                                <xsd:appinfo>
                                    <ann:Glossary/>
                                    <ann:Status current="Active"/>
                                </xsd:appinfo>
                            </xsd:annotation>
                        </xsd:element>
                    </xsd:sequence>
                </xsd:complexType>
            </xsd:element>

            <xsd:element name="ENA_LINK">
                <xsd:complexType>
                    <xsd:sequence>
                        <xsd:element name="DB" type="xs:string" minOccurs="1" maxOccurs="1">
                            <xsd:annotation>
                                <xsd:documentation> EBI ENA controlled vocabulary of permitted cross references. </xsd:documentation>
                                <xsd:appinfo>
                                    <ann:Glossary/>
                                    <ann:Status current="Active"/>
                                </xsd:appinfo>
                            </xsd:annotation>
                        </xsd:element>
                        <xsd:element name="ID" minOccurs="1" maxOccurs="1" type="xs:string">
                            <xsd:annotation>
                                <xsd:documentation> Accession in the referenced database.</xsd:documentation>
                                <xsd:appinfo>
                                    <ann:Glossary/>
                                    <ann:Status current="Active"/>
                                </xsd:appinfo>
                            </xsd:annotation>
                        </xsd:element>
                        <xsd:element name="URL" minOccurs="0" maxOccurs="1" type="xsd:anyURI">
                            <xsd:annotation>
                                <xsd:documentation>An URL to the cross-references accession. </xsd:documentation>
                                <xsd:appinfo>
                                    <ann:Glossary/>
                                    <ann:Status current="Active"/>
                                </xsd:appinfo>
                            </xsd:annotation>
                        </xsd:element>
                        <xsd:element name="LABEL" type="xs:string" minOccurs="0" maxOccurs="1">
                            <xsd:annotation>
                                <xsd:documentation>A textual description of the cross-reference. </xsd:documentation>
                                <xsd:appinfo>
                                    <ann:Glossary/>
                                    <ann:Status current="Active"/>
                                </xsd:appinfo>
                            </xsd:annotation>
                        </xsd:element>
                    </xsd:sequence>
                </xsd:complexType>
            </xsd:element>

        </xsd:choice>
    </xsd:complexType>

    <xsd:complexType name="PoolMemberType">
        <xsd:annotation>
            <xsd:documentation> Impementation of lookup table between Sample Pool member and identified read_group_tags for a given
                READ_LABEL </xsd:documentation>
            <xsd:appinfo>
                <ann:Glossary/>
                <ann:Status current="Active"/>
            </xsd:appinfo>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element name="IDENTIFIERS" type="com:IdentifierType" minOccurs="0" maxOccurs="1">
                <xsd:annotation>
                    <xsd:documentation> Set of reference IDs to parent experiment record. This block is intended to replace the use
                        of the less structured RefNameGroup identifiers. </xsd:documentation>
                    <xsd:appinfo>
                        <ann:Status current="Active"/>
                    </xsd:appinfo>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="READ_LABEL" minOccurs="0" maxOccurs="unbounded">
                <xsd:complexType>
                    <xsd:simpleContent>
                        <xsd:extension base="xs:string">
                            <xsd:attribute name="read_group_tag" type="xs:string">
                                <xsd:annotation>
                                    <xsd:documentation> Assignment of read_group_tag to decoded read </xsd:documentation>
                                    <xsd:appinfo>
                                        <ann:Glossary/>
                                        <ann:Status current="Active"/>
                                    </xsd:appinfo>
                                </xsd:annotation>
                            </xsd:attribute>
                        </xsd:extension>
                    </xsd:simpleContent>
                </xsd:complexType>
            </xsd:element>
        </xsd:sequence>
        <xsd:attributeGroup ref="com:RefNameGroup"/>
        <xsd:attribute name="member_name" type="xs:string" use="optional">
            <xsd:annotation>
                <xsd:documentation> Label a sample within a scope of the pool </xsd:documentation>
                <xs:appinfo>
                    <ann:Glossary/>
                    <ann:Status current="Active"/>
                </xs:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="proportion" type="xs:float" use="optional">
            <xsd:annotation>
                <xsd:documentation> Proportion of this sample (in percent) that was included in sample pool. </xsd:documentation>
                <xs:appinfo>
                    <ann:Glossary/>
                    <ann:Status current="Active"/>
                </xs:appinfo>
            </xsd:annotation>
        </xsd:attribute>
    </xsd:complexType>

    <xsd:complexType name="SampleDescriptorType">
        <xsd:annotation>
            <xsd:documentation> The SAMPLE_DESCRIPTOR specifies how to decode the individual reads of interest from the monolithic
                spot sequence. The spot descriptor contains aspects of the experimental design, platform, and processing
                information. There will be two methods of specification: one will be an index into a table of typical decodings, the
                other being an exact specification. </xsd:documentation>
            <xsd:appinfo>
                <ann:Glossary/>
                <ann:Status current="Active"/>
            </xsd:appinfo>
        </xsd:annotation>
        <xsd:choice minOccurs="0" maxOccurs="1">
            <xsd:element name="IDENTIFIERS" type="com:IdentifierType">
                <xsd:annotation>
                    <xsd:documentation> Set of reference IDs to parent experiment record. This block is intended to replace the use
                        of the less structured RefNameGroup identifiers. </xsd:documentation>
                    <xsd:appinfo>
                        <ann:Status current="Active"/>
                    </xsd:appinfo>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="POOL">
                <xsd:annotation>
                    <xsd:documentation> Identifies a list of group/pool/multiplex sample members. This implies that this sample
                        record is a group, pool, or multiplex, but is continues to receive its own accession and can be referenced
                        by an experiment. By default if no match to any of the listed members can be determined, then the default
                        sampel reference is used. </xsd:documentation>
                    <xsd:appinfo>
                        <ann:Glossary/>
                        <ann:Status current="Active"/>
                    </xsd:appinfo>
                </xsd:annotation>
                <xsd:complexType>
                    <xsd:sequence>
                        <xsd:element name="DEFAULT_MEMBER" type="com:PoolMemberType" minOccurs="0" maxOccurs="1">
                            <xsd:annotation>
                                <xsd:documentation> Reference to the sample that is used when read membership cannot be determined.
                                    A default member should be provided if there exists a possibility that some reads will be left
                                    over from barcode/MID resolution. A default member is not needed when defining a true pool
                                    (where individual samples are not distinguished in the reads), or the reads have been
                                    partitioned among the pool members (no leftovers). </xsd:documentation>
                                <xsd:appinfo>
                                    <ann:Glossary/>
                                    <ann:Status current="Active"/>
                                </xsd:appinfo>
                            </xsd:annotation>
                        </xsd:element>
                        <xsd:element name="MEMBER" type="com:PoolMemberType" minOccurs="1" maxOccurs="unbounded">
                            <xsd:annotation>
                                <xsd:documentation> Reference to the sample as determined from barcode/MID resolution or read
                                    partition. </xsd:documentation>
                                <xsd:appinfo>
                                    <ann:Glossary/>
                                    <ann:Status current="Active"/>
                                </xsd:appinfo>
                            </xsd:annotation>
                        </xsd:element>
                    </xsd:sequence>
                </xsd:complexType>
            </xsd:element>

        </xsd:choice>

        <xsd:attributeGroup ref="com:RefNameGroup"/>

    </xsd:complexType>
    <xsd:complexType name="LibraryDescriptorType">
        <xsd:annotation>
            <xsd:documentation> The LIBRARY_DESCRIPTOR specifies the origin of the material being sequenced and any treatments that
                the material might have undergone that affect the sequencing result. This specification is needed even if the
                platform does not require a library construction step per se. </xsd:documentation>
            <xsd:appinfo>
                <ann:Glossary/>
                <ann:Status current="Active"/>
            </xsd:appinfo>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element name="LIBRARY_NAME" type="xs:string" maxOccurs="1" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation> The submitter's name for this library. </xsd:documentation>
                    <xsd:appinfo>
                        <ann:Glossary/>
                        <ann:Status current="Active"/>
                    </xsd:appinfo>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="LIBRARY_STRATEGY" maxOccurs="1" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation> Sequencing technique intended for this library. </xsd:documentation>
                    <xsd:appinfo>
                        <ann:Glossary/>
                        <ann:Status current="Active"/>
                    </xsd:appinfo>
                </xsd:annotation>
                <xsd:simpleType>
                    <xsd:restriction base="xs:string">
                        <xsd:enumeration value="WGS">
                            <xsd:annotation>
                                <xsd:documentation> Random sequencing of the whole genome. </xsd:documentation>
                                <xs:appinfo>
                                    <ann:Glossary/>
                                    <ann:Status current="Active"/>
                                    <ann:DocumentationLinks>
                                        <XREF_LINK>
                                            <DB>pubmed</DB>
                                            <ID>10731132</ID>
                                        </XREF_LINK>
                                    </ann:DocumentationLinks>
                                </xs:appinfo>
                            </xsd:annotation>
                        </xsd:enumeration>
                        <xsd:enumeration value="WGA">
                            <xsd:annotation>
                                <xsd:documentation> Whole Genome Amplification followed by random sequencing. </xsd:documentation>
                                <xs:appinfo>
                                    <ann:Glossary/>
                                    <ann:Status current="Active"/>
                                    <ann:DocumentationLinks>
                                        <XREF_LINK>
                                            <DB>pubmed</DB>
                                            <ID>1631067</ID>
                                        </XREF_LINK>
                                        <XREF_LINK>
                                            <DB>pubmed</DB>
                                            <ID>8962113</ID>
                                        </XREF_LINK>
                                    </ann:DocumentationLinks>
                                </xs:appinfo>
                            </xsd:annotation>
                        </xsd:enumeration>
                        <xsd:enumeration value="WXS">
                            <xsd:annotation>
                                <xsd:documentation> Random sequencing of exonic regions selected from the genome. </xsd:documentation>
                                <xs:appinfo>
                                    <ann:Glossary/>
                                    <ann:Status current="Active"/>
                                    <ann:DocumentationLinks>
                                        <XREF_LINK>
                                            <DB>pubmed</DB>
                                            <ID>20111037</ID>
                                        </XREF_LINK>
                                    </ann:DocumentationLinks>
                                </xs:appinfo>
                            </xsd:annotation>
                        </xsd:enumeration>
                        <xsd:enumeration value="RNA-Seq">
                            <xsd:annotation>
                                <xsd:documentation> Random sequencing of whole transcriptome, also known as Whole Transcriptome
                                    Shotgun Sequencing, or WTSS).</xsd:documentation>
                                <xs:appinfo>
                                    <ann:Glossary/>
                                    <ann:Status current="Active"/>
                                    <ann:DocumentationLinks>
                                        <XREF_LINK>
                                            <DB>pubmed</DB>
                                            <ID>18611170</ID>
                                        </XREF_LINK>
                                    </ann:DocumentationLinks>
                                </xs:appinfo>
                            </xsd:annotation>
                        </xsd:enumeration>
                        <xsd:enumeration value="miRNA-Seq">
                            <xsd:annotation>
                                <xsd:documentation> Micro RNA sequencing strategy designed to capture post-transcriptional RNA
                                    elements and include non-coding functional elements.</xsd:documentation>
                                <xs:appinfo>
                                    <ann:Glossary/>
                                    <ann:Status current="Active"/>
                                    <ann:DocumentationLinks>
                                        <XREF_LINK>
                                            <DB>pubmed</DB>
                                            <ID>21787409</ID>
                                        </XREF_LINK>
                                    </ann:DocumentationLinks>
                                </xs:appinfo>
                            </xsd:annotation>
                        </xsd:enumeration>
                        <xsd:enumeration value="WCS">
                            <xsd:annotation>
                                <xsd:documentation> Random sequencing of a whole chromosome or other replicon isolated from a
                                    genome. </xsd:documentation>
                                <xs:appinfo>
                                    <ann:Glossary/>
                                    <ann:Status current="Active"/>
                                </xs:appinfo>
                            </xsd:annotation>
                        </xsd:enumeration>
                        <xsd:enumeration value="CLONE">
                            <xsd:annotation>
                                <xsd:documentation> Genomic clone based (hierarchical) sequencing. </xsd:documentation>
                                <xs:appinfo>
                                    <ann:Glossary/>
                                    <ann:Status current="Active"/>
                                </xs:appinfo>
                            </xsd:annotation>
                        </xsd:enumeration>
                        <xsd:enumeration value="POOLCLONE">
                            <xsd:annotation>
                                <xsd:documentation> Shotgun of pooled clones (usually BACs and Fosmids). </xsd:documentation>
                                <xs:appinfo>
                                    <ann:Glossary/>
                                    <ann:Status current="Active"/>
                                </xs:appinfo>
                            </xsd:annotation>
                        </xsd:enumeration>
                        <xsd:enumeration value="AMPLICON">
                            <xsd:annotation>
                                <xsd:documentation> Sequencing of overlapping or distinct PCR or RT-PCR products. For example,
                                    metagenomic community profiling using SSU rRNA . </xsd:documentation>
                                <xs:appinfo>
                                    <ann:Glossary/>
                                    <ann:Status current="Active"/>
                                    <ann:DocumentationLinks>
                                        <XREF_LINK>
                                            <DB>pubmed</DB>
                                            <ID>19023400</ID>
                                        </XREF_LINK>
                                    </ann:DocumentationLinks>
                                </xs:appinfo>
                            </xsd:annotation>
                        </xsd:enumeration>
                        <xsd:enumeration value="CLONEEND">
                            <xsd:annotation>
                                <xsd:documentation> Clone end (5', 3', or both) sequencing. </xsd:documentation>
                                <xs:appinfo>
                                    <ann:Glossary/>
                                    <ann:Status current="Active"/>
                                    <ann:DocumentationLinks>
                                        <XREF_LINK>
                                            <DB>pubmed</DB>
                                            <ID>18836033</ID>
                                        </XREF_LINK>
                                    </ann:DocumentationLinks>
                                </xs:appinfo>
                            </xsd:annotation>
                        </xsd:enumeration>
                        <xsd:enumeration value="FINISHING">
                            <xsd:annotation>
                                <xsd:documentation> Sequencing intended to finish (close) gaps in existing coverage. </xsd:documentation>
                                <xs:appinfo>
                                    <ann:Glossary/>
                                    <ann:Status current="Active"/>
                                </xs:appinfo>
                            </xsd:annotation>
                        </xsd:enumeration>
                        <xsd:enumeration value="ChIP-Seq">
                            <xsd:annotation>
                                <xsd:documentation> Direct sequencing of chromatin immunoprecipitates. </xsd:documentation>
                                <xs:appinfo>
                                    <ann:Glossary/>
                                    <ann:Status current="Active"/>
                                    <ann:DocumentationLinks>
                                        <XREF_LINK>
                                            <DB>pubmed</DB>
                                            <ID>18684996</ID>
                                        </XREF_LINK>
                                        <XREF_LINK>
                                            <DB>pubmed</DB>
                                            <ID>20204874</ID>
                                        </XREF_LINK>
                                    </ann:DocumentationLinks>
                                </xs:appinfo>
                            </xsd:annotation>
                        </xsd:enumeration>
                        <xsd:enumeration value="MNase-Seq">
                            <xsd:annotation>
                                <xsd:documentation> Direct sequencing following MNase digestion. </xsd:documentation>
                                <xs:appinfo>
                                    <ann:Glossary/>
                                    <ann:Status current="Active"/>
                                </xs:appinfo>
                            </xsd:annotation>
                        </xsd:enumeration>
                        <xsd:enumeration value="DNase-Hypersensitivity">
                            <xsd:annotation>
                                <xsd:documentation> Sequencing of hypersensitive sites, or segments of open chromatin that are more
                                    readily cleaved by DNaseI. </xsd:documentation>
                                <xs:appinfo>
                                    <ann:Glossary/>
                                    <ann:Status current="Active"/>
                                </xs:appinfo>
                            </xsd:annotation>
                        </xsd:enumeration>
                        <xsd:enumeration value="Bisulfite-Seq">
                            <xsd:annotation>
                                <xsd:documentation>MethylC-seq. Sequencing following treatment of DNA with bisulfite to convert
                                    cytosine residues to uracil depending on methylation status. </xsd:documentation>
                                <xs:appinfo>
                                    <ann:Glossary/>
                                    <ann:Status current="Active"/>
                                    <ann:DocumentationLinks>
                                        <XREF_LINK>
                                            <DB>pubmed</DB>
                                            <ID>19829295</ID>
                                        </XREF_LINK>
                                    </ann:DocumentationLinks>
                                </xs:appinfo>
                            </xsd:annotation>
                        </xsd:enumeration>
                        <xsd:enumeration value="EST">
                            <xsd:annotation>
                                <xsd:documentation> Single pass sequencing of cDNA templates </xsd:documentation>
                                <xs:appinfo>
                                    <ann:Glossary/>
                                    <ann:Status current="Active"/>
                                </xs:appinfo>
                            </xsd:annotation>
                        </xsd:enumeration>
                        <xsd:enumeration value="FL-cDNA">
                            <xsd:annotation>
                                <xsd:documentation> Full-length sequencing of cDNA templates </xsd:documentation>
                                <xs:appinfo>
                                    <ann:Glossary/>
                                    <ann:Status current="Active"/>
                                </xs:appinfo>
                            </xsd:annotation>
                        </xsd:enumeration>
                        <xsd:enumeration value="CTS">
                            <xsd:annotation>
                                <xsd:documentation> Concatenated Tag Sequencing </xsd:documentation>
                                <xs:appinfo>
                                    <ann:Glossary/>
                                    <ann:Status current="Active"/>
                                </xs:appinfo>
                            </xsd:annotation>
                        </xsd:enumeration>
                        <xsd:enumeration value="MRE-Seq">
                            <xsd:annotation>
                                <xsd:documentation> Methylation-Sensitive Restriction Enzyme Sequencing strategy. </xsd:documentation>
                                <xs:appinfo>
                                    <ann:Glossary/>
                                    <ann:Status current="Active"/>
                                </xs:appinfo>
                            </xsd:annotation>
                        </xsd:enumeration>
                        <xs:enumeration value="MeDIP-Seq">
                            <xsd:annotation>
                                <xsd:documentation> Methylated DNA Immunoprecipitation Sequencing strategy. </xsd:documentation>
                                <xs:appinfo>
                                    <ann:Glossary/>
                                    <ann:Status current="Active"/>
                                </xs:appinfo>
                            </xsd:annotation>
                        </xs:enumeration>
                        <xsd:enumeration value="MBD-Seq">
                            <xsd:annotation>
                                <xsd:documentation> Direct sequencing of methylated fractions sequencing strategy. </xsd:documentation>
                                <xs:appinfo>
                                    <ann:Glossary/>
                                    <ann:Status current="Active"/>
                                </xs:appinfo>
                            </xsd:annotation>
                        </xsd:enumeration>
                        <xsd:enumeration value="Tn-Seq">
                            <xsd:annotation>
                                <xsd:documentation>Quantitatively determine fitness of bacterial genes based 
                                    on how many times a purposely seeded transposon gets inserted into each 
                                    gene of a colony after some time. </xsd:documentation>
                                <xs:appinfo>
                                    <ann:Glossary/>
                                    <ann:Status current="Active"/>
                                    <ann:DocumentationLinks>
                                        <XREF_LINK>
                                            <DB>pubmed</DB>
                                            <ID>19767758</ID>
                                        </XREF_LINK>
                                    </ann:DocumentationLinks>
                                </xs:appinfo>
                            </xsd:annotation>
                        </xsd:enumeration>
                        <xsd:enumeration value="OTHER">
                            <xsd:annotation>
                                <xsd:documentation> Library strategy not listed. </xsd:documentation>
                                <xs:appinfo>
                                    <ann:Glossary/>
                                    <ann:Status current="Active"/>
                                </xs:appinfo>
                            </xsd:annotation>
                        </xsd:enumeration>
                    </xsd:restriction>
                </xsd:simpleType>
            </xsd:element>
            <xsd:element name="LIBRARY_SOURCE">
                <xsd:annotation>
                    <xsd:documentation> The LIBRARY_SOURCE specifies the type of source material that is being sequenced. </xsd:documentation>
                    <xsd:appinfo>
                        <ann:Glossary/>
                        <ann:Status current="Active"/>
                    </xsd:appinfo>
                </xsd:annotation>
                <xsd:simpleType>
                    <xsd:restriction base="xs:string">
                        <xsd:enumeration value="GENOMIC">
                            <xsd:annotation>
                                <xsd:documentation> Genomic DNA (includes PCR products from genomic DNA). </xsd:documentation>
                                <xs:appinfo>
                                    <ann:Glossary/>
                                    <ann:Status current="Active"/>
                                </xs:appinfo>
                            </xsd:annotation>
                        </xsd:enumeration>
                        <xsd:enumeration value="TRANSCRIPTOMIC">
                            <xsd:annotation>
                                <xsd:documentation> Transcription products or non genomic DNA (EST, cDNA, RT-PCR, screened
                                    libraries). </xsd:documentation>
                                <xs:appinfo>
                                    <ann:Glossary/>
                                    <ann:Status current="Active"/>
                                </xs:appinfo>
                            </xsd:annotation>
                        </xsd:enumeration>
                        <xsd:enumeration value="METAGENOMIC">
                            <xsd:annotation>
                                <xsd:documentation> Mixed material from metagenome. </xsd:documentation>
                                <xs:appinfo>
                                    <ann:Glossary/>
                                    <ann:Status current="Active"/>
                                </xs:appinfo>
                            </xsd:annotation>
                        </xsd:enumeration>
                        <xsd:enumeration value="METATRANSCRIPTOMIC">
                            <xsd:annotation>
                                <xsd:documentation> Transcription products from community targets </xsd:documentation>
                                <xs:appinfo>
                                    <ann:Glossary/>
                                    <ann:Status current="Active"/>
                                </xs:appinfo>
                            </xsd:annotation>
                        </xsd:enumeration>
                        <xsd:enumeration value="NON GENOMIC">
                            <xsd:annotation>
                                <xsd:documentation> Deprecated. Use TRANSCRIPTOMIC or METAGENOMIC. </xsd:documentation>
                                <xsd:appinfo>
                                    <ann:Glossary/>
                                    <ann:Status current="Deprecated"/>
                                </xsd:appinfo>
                            </xsd:annotation>
                        </xsd:enumeration>
                        <xsd:enumeration value="SYNTHETIC">
                            <xsd:annotation>
                                <xsd:documentation> Synthetic DNA. </xsd:documentation>
                                <xs:appinfo>
                                    <ann:Glossary/>
                                    <ann:Status current="Active"/>
                                </xs:appinfo>
                            </xsd:annotation>
                        </xsd:enumeration>
                        <xsd:enumeration value="VIRAL RNA">
                            <xsd:annotation>
                                <xsd:documentation> Viral RNA. </xsd:documentation>
                                <xs:appinfo>
                                    <ann:Glossary/>
                                    <ann:Status current="Active"/>
                                </xs:appinfo>
                            </xsd:annotation>
                        </xsd:enumeration>
                        <xsd:enumeration value="OTHER">
                            <xsd:annotation>
                                <xsd:documentation> Other, unspecified, or unknown library source material. </xsd:documentation>
                                <xs:appinfo>
                                    <ann:Glossary/>
                                    <ann:Status current="Active"/>
                                </xs:appinfo>
                            </xsd:annotation>
                        </xsd:enumeration>
                    </xsd:restriction>
                </xsd:simpleType>
            </xsd:element>
            <xsd:element name="LIBRARY_SELECTION">
                <xsd:annotation>
                    <xsd:documentation> Whether any method was used to select for or against, enrich, or screen the material being
                        sequenced. </xsd:documentation>
                    <xsd:appinfo>
                        <ann:Glossary/>
                        <ann:Status current="Active"/>
                    </xsd:appinfo>
                </xsd:annotation>
                <xsd:simpleType>
                    <xsd:restriction base="xs:string">
                        <xsd:enumeration value="RANDOM">
                            <xsd:annotation>
                                <xsd:documentation>Random selection by shearing or other method.</xsd:documentation>
                                <xs:appinfo>
                                    <ann:Glossary/>
                                    <ann:Status current="Active"/>
                                </xs:appinfo>
                            </xsd:annotation>
                        </xsd:enumeration>
                        <xsd:enumeration value="PCR">
                            <xsd:annotation>
                                <xsd:documentation>Source material was selected by designed primers.</xsd:documentation>
                                <xs:appinfo>
                                    <ann:Glossary/>
                                    <ann:Status current="Active"/>
                                </xs:appinfo>
                            </xsd:annotation>
                        </xsd:enumeration>
                        <xsd:enumeration value="RANDOM PCR">
                            <xsd:annotation>
                                <xsd:documentation>Source material was selected by randomly generated primers.</xsd:documentation>
                                <xs:appinfo>
                                    <ann:Glossary/>
                                    <ann:Status current="Active"/>
                                </xs:appinfo>
                            </xsd:annotation>
                        </xsd:enumeration>
                        <xsd:enumeration value="RT-PCR">
                            <xsd:annotation>
                                <xsd:documentation>Source material was selected by reverse transcription PCR</xsd:documentation>
                                <xs:appinfo>
                                    <ann:Glossary/>
                                    <ann:Status current="Active"/>
                                </xs:appinfo>
                            </xsd:annotation>
                        </xsd:enumeration>
                        <xsd:enumeration value="HMPR">
                            <xsd:annotation>
                                <xsd:documentation>Hypo-methylated partial restriction digest</xsd:documentation>
                                <xs:appinfo>
                                    <ann:Glossary/>
                                    <ann:Status current="Active"/>
                                </xs:appinfo>
                            </xsd:annotation>
                        </xsd:enumeration>
                        <xsd:enumeration value="MF">
                            <xsd:annotation>
                                <xsd:documentation>Methyl Filtrated</xsd:documentation>
                                <xs:appinfo>
                                    <ann:Glossary/>
                                    <ann:Status current="Active"/>
                                </xs:appinfo>
                            </xsd:annotation>
                        </xsd:enumeration>
                        <xsd:enumeration value="CF-S">
                            <xsd:annotation>
                                <xsd:documentation>Cot-filtered single/low-copy genomic DNA</xsd:documentation>
                                <xs:appinfo>
                                    <ann:Glossary/>
                                    <ann:Status current="Active"/>
                                </xs:appinfo>
                            </xsd:annotation>
                        </xsd:enumeration>
                        <xsd:enumeration value="CF-M">
                            <xsd:annotation>
                                <xsd:documentation>Cot-filtered moderately repetitive genomic DNA</xsd:documentation>
                                <xs:appinfo>
                                    <ann:Glossary/>
                                    <ann:Status current="Active"/>
                                </xs:appinfo>
                            </xsd:annotation>
                        </xsd:enumeration>
                        <xsd:enumeration value="CF-H">
                            <xsd:annotation>
                                <xsd:documentation>Cot-filtered highly repetitive genomic DNA</xsd:documentation>
                                <xs:appinfo>
                                    <ann:Glossary/>
                                    <ann:Status current="Active"/>
                                </xs:appinfo>
                            </xsd:annotation>
                        </xsd:enumeration>
                        <xsd:enumeration value="CF-T">
                            <xsd:annotation>
                                <xsd:documentation>Cot-filtered theoretical single-copy genomic DNA</xsd:documentation>
                                <xs:appinfo>
                                    <ann:Glossary/>
                                    <ann:Status current="Active"/>
                                </xs:appinfo>
                            </xsd:annotation>
                        </xsd:enumeration>
                        <xsd:enumeration value="MSLL">
                            <xsd:annotation>
                                <xsd:documentation>Methylation Spanning Linking Library</xsd:documentation>
                                <xs:appinfo>
                                    <ann:Glossary/>
                                    <ann:Status current="Active"/>
                                </xs:appinfo>
                            </xsd:annotation>
                        </xsd:enumeration>
                        <xsd:enumeration value="cDNA">
                            <xsd:annotation>
                                <xsd:documentation>PolyA selection or enrichment for messenger RNA (mRNA). complementary DNA. </xsd:documentation>
                                <xs:appinfo>
                                    <ann:Glossary/>
                                    <ann:Status current="Active"/>
                                </xs:appinfo>
                            </xsd:annotation>
                        </xsd:enumeration>
                        <xsd:enumeration value="ChIP">
                            <xsd:annotation>
                                <xsd:documentation>Chromatin immunoprecipitation</xsd:documentation>
                                <xs:appinfo>
                                    <ann:Glossary/>
                                    <ann:Status current="Active"/>
                                </xs:appinfo>
                            </xsd:annotation>
                        </xsd:enumeration>
                        <xsd:enumeration value="MNase">
                            <xsd:annotation>
                                <xsd:documentation>Micrococcal Nuclease (MNase) digestion</xsd:documentation>
                                <xs:appinfo>
                                    <ann:Glossary/>
                                    <ann:Status current="Active"/>
                                </xs:appinfo>
                            </xsd:annotation>
                        </xsd:enumeration>
                        <xsd:enumeration value="DNAse">
                            <xsd:annotation>
                                <xsd:documentation>Deoxyribonuclease (MNase) digestion</xsd:documentation>
                                <xs:appinfo>
                                    <ann:Glossary/>
                                    <ann:Status current="Active"/>
                                </xs:appinfo>
                            </xsd:annotation>
                        </xsd:enumeration>
                        <xsd:enumeration value="Hybrid Selection">
                            <xsd:annotation>
                                <xsd:documentation>Selection by hybridization in array or solution.</xsd:documentation>
                                <xs:appinfo>
                                    <ann:Glossary/>
                                    <ann:Status current="Active"/>
                                </xs:appinfo>
                            </xsd:annotation>
                        </xsd:enumeration>
                        <xsd:enumeration value="Reduced Representation">
                            <xsd:annotation>
                                <xsd:documentation>Reproducible genomic subsets, often generated by restriction fragment size
                                    selection, containing a manageable number of loci to facilitate re-sampling. </xsd:documentation>
                                <xs:appinfo>
                                    <ann:Glossary/>
                                    <ann:Status current="Active"/>
                                </xs:appinfo>
                            </xsd:annotation>
                        </xsd:enumeration>
                        <xsd:enumeration value="Restriction Digest">
                            <xsd:annotation>
                                <xsd:documentation> DNA fractionation using restriction enzymes. </xsd:documentation>
                                <xs:appinfo>
                                    <ann:Glossary/>
                                    <ann:Status current="Active"/>
                                </xs:appinfo>
                            </xsd:annotation>
                        </xsd:enumeration>
                        <xsd:enumeration value="5-methylcytidine antibody">
                            <xsd:annotation>
                                <xsd:documentation> Selection of methylated DNA fragments using an antibody raised against
                                    5-methylcytosine or 5-methylcytidine (m5C). </xsd:documentation>
                                <xs:appinfo>
                                    <ann:Glossary/>
                                    <ann:Status current="Active"/>
                                </xs:appinfo>
                            </xsd:annotation>
                        </xsd:enumeration>
                        <xsd:enumeration value="MBD2 protein methyl-CpG binding domain">
                            <xsd:annotation>
                                <xsd:documentation> Enrichment by methyl-CpG binding domain. </xsd:documentation>
                                <xs:appinfo>
                                    <ann:Glossary/>
                                    <ann:Status current="Active"/>
                                </xs:appinfo>
                            </xsd:annotation>
                        </xsd:enumeration>
                        <xsd:enumeration value="CAGE">
                            <xsd:annotation>
                                <xsd:documentation> Cap-analysis gene expression. </xsd:documentation>
                                <xs:appinfo>
                                    <ann:Glossary/>
                                    <ann:Status current="Active"/>
                                    <ann:DocumentationLinks>
                                        <XREF_LINK>
                                            <DB>pubmed</DB>
                                            <ID>14663149</ID>
                                        </XREF_LINK>
                                    </ann:DocumentationLinks>
                                </xs:appinfo>
                            </xsd:annotation>
                        </xsd:enumeration>
                        <xsd:enumeration value="RACE">
                            <xsd:annotation>
                                <xsd:documentation> Rapid Amplification of cDNA Ends. </xsd:documentation>
                                <xs:appinfo>
                                    <ann:Glossary/>
                                    <ann:Status current="Active"/>
                                    <ann:DocumentationLinks>
                                        <XREF_LINK>
                                            <DB>pubmed</DB>
                                            <ID>2461560</ID>
                                        </XREF_LINK>
                                    </ann:DocumentationLinks>
                                </xs:appinfo>
                            </xsd:annotation>
                        </xsd:enumeration>
                        <xsd:enumeration value="size fractionation">
                            <xsd:annotation>
                                <xsd:documentation> Physical selection of size appropriate targets. </xsd:documentation>
                                <xs:appinfo>
                                    <ann:Glossary/>
                                    <ann:Status current="Active"/>
                                </xs:appinfo>
                            </xsd:annotation>
                        </xsd:enumeration>
                        <xsd:enumeration value="MDA">
                            <xsd:annotation>
                                <xsd:documentation> Multiple Displacement Amplification, a non-PCR based DNA amplification technique
                                    that amplifies a minute quantifies of DNA to levels suitable for genomic analysis. </xsd:documentation>
                                <xs:appinfo>
                                    <ann:Glossary/>
                                    <ann:Status current="Active"/>
                                    <ann:DocumentationLinks>
                                        <XREF_LINK>
                                            <DB>pubmed</DB>
                                            <ID>18793430</ID>
                                        </XREF_LINK>
                                        <XREF_LINK>
                                            <DB>pubmed</DB>
                                            <ID>17487184</ID>
                                        </XREF_LINK>
                                    </ann:DocumentationLinks>
                                </xs:appinfo>
                            </xsd:annotation>
                        </xsd:enumeration>
                        <xsd:enumeration value="padlock probes capture method">
                            <xsd:annotation>
                                <xsd:documentation> Targeted sequence capture protocol covering an arbitrary set of nonrepetitive
                                    genomics targets. An example is capture bisulfite sequencing using padlock probes (BSPP). </xsd:documentation>
                                <xs:appinfo>
                                    <ann:Glossary/>
                                    <ann:Status current="Active"/>
                                    <ann:DocumentationLinks>
                                        <XREF_LINK>
                                            <DB>pubmed</DB>
                                            <ID>19330000</ID>
                                        </XREF_LINK>
                                    </ann:DocumentationLinks>
                                </xs:appinfo>
                            </xsd:annotation>
                        </xsd:enumeration>
                        <xsd:enumeration value="other">
                            <xsd:annotation>
                                <xsd:documentation> Other library enrichment, screening, or selection process. </xsd:documentation>
                                <xs:appinfo>
                                    <ann:Glossary/>
                                    <ann:Status current="Active"/>
                                </xs:appinfo>
                            </xsd:annotation>
                        </xsd:enumeration>
                        <xsd:enumeration value="unspecified">
                            <xsd:annotation>
                                <xsd:documentation> Library enrichment, screening, or selection is not specified. </xsd:documentation>
                                <xs:appinfo>
                                    <ann:Glossary/>
                                    <ann:Status current="Active"/>
                                </xs:appinfo>
                            </xsd:annotation>
                        </xsd:enumeration>
                    </xsd:restriction>
                </xsd:simpleType>
            </xsd:element>
            <xsd:element name="LIBRARY_LAYOUT">
                <xsd:annotation>
                    <xsd:documentation> LIBRARY_LAYOUT specifies whether to expect single, paired, or other configuration of reads.
                        In the case of paired reads, information about the relative distance and orientation is specified. </xsd:documentation>
                    <xsd:appinfo>
                        <ann:Glossary/>
                        <ann:Status current="Active"/>
                    </xsd:appinfo>
                </xsd:annotation>
                <xsd:complexType>
                    <xsd:choice>
                        <xsd:element name="SINGLE">
                            <xsd:complexType>
                                <xsd:annotation>
                                    <xsd:documentation> Reads are unpaired (usual case). </xsd:documentation>
                                    <xsd:appinfo>
                                        <ann:Glossary/>
                                        <ann:Status current="Active"/>
                                    </xsd:appinfo>
                                </xsd:annotation>
                            </xsd:complexType>
                        </xsd:element>
                        <xsd:element name="PAIRED">
                            <xsd:complexType>
                                <xsd:attribute name="ORIENTATION" type="xs:string">
                                    <xsd:annotation>
                                        <xsd:documentation> </xsd:documentation>
                                        <xsd:appinfo>
                                            <ann:Glossary/>
                                            <ann:Status current="Active"/>
                                        </xsd:appinfo>
                                    </xsd:annotation>
                                </xsd:attribute>
                                <xsd:attribute name="NOMINAL_LENGTH" type="xsd:nonNegativeInteger">
                                    <xsd:annotation>
                                        <xsd:documentation> </xsd:documentation>
                                        <xsd:appinfo>
                                            <ann:Glossary/>
                                            <ann:Status current="Active"/>
                                        </xsd:appinfo>
                                    </xsd:annotation>
                                </xsd:attribute>
                                <xsd:attribute name="NOMINAL_SDEV" type="xsd:double">
                                    <xsd:annotation>
                                        <xsd:documentation> </xsd:documentation>
                                        <xsd:appinfo>
                                            <ann:Glossary/>
                                            <ann:Status current="Active"/>
                                        </xsd:appinfo>
                                    </xsd:annotation>
                                </xsd:attribute>
                            </xsd:complexType>
                        </xsd:element>
                    </xsd:choice>
                </xsd:complexType>
            </xsd:element>
            <xsd:element name="TARGETED_LOCI" minOccurs="0" maxOccurs="1">
                <xsd:complexType>
                    <xsd:annotation>
                        <xsd:documentation> Names the gene(s) or locus(loci) or other genomic feature(s) targeted by the sequence. </xsd:documentation>
                        <xsd:appinfo>
                            <ann:Glossary/>
                            <ann:Status current="Active"/>
                        </xsd:appinfo>
                    </xsd:annotation>
                    <xsd:sequence>
                        <xsd:element name="LOCUS" maxOccurs="unbounded" minOccurs="1">
                            <xsd:complexType>
                                <xsd:sequence>
                                    <xsd:element name="PROBE_SET" type="com:XRefType" maxOccurs="1" minOccurs="0">
                                        <xsd:annotation>
                                            <xsd:documentation> Reference to an archived primer or probe set. Example: dbProbe </xsd:documentation>
                                            <xsd:appinfo>
                                                <ann:Glossary/>
                                                <ann:Status current="Active"/>
                                            </xsd:appinfo>
                                        </xsd:annotation>
                                    </xsd:element>
                                </xsd:sequence>
                                <xsd:attribute name="locus_name">
                                    <xsd:simpleType>
                                        <xsd:restriction base="xs:string">
                                            <xsd:enumeration value="16S rRNA">
                                                <xsd:annotation>
                                                    <xsd:documentation> Bacterial small subunit ribosomal RNA, a locus used for
                                                        phylogenetic studies of bacteria and as a target for random target PCR in
                                                        environmental biodiversity screening. </xsd:documentation>
                                                    <xs:appinfo>
                                                        <ann:Glossary/>
                                                        <ann:Status current="Active"/>
                                                    </xs:appinfo>
                                                </xsd:annotation>
                                            </xsd:enumeration>
                                            <xsd:enumeration value="18S rRNA">
                                                <xsd:annotation>
                                                    <xsd:documentation> Eukaryotic small subunit ribosomal RNA, a locus used for
                                                        phylogenetic studies of eukaryotes and as a target for random target PCR in
                                                        environmental biodiversity screening. </xsd:documentation>
                                                    <xs:appinfo>
                                                        <ann:Glossary/>
                                                        <ann:Status current="Active"/>
                                                    </xs:appinfo>
                                                </xsd:annotation>
                                            </xsd:enumeration>
                                            <xsd:enumeration value="RBCL">
                                                <xsd:annotation>
                                                    <xsd:documentation> RuBisCO large subunit : ribulose-1,5-bisphosphate
                                                        carboxylase/oxygenase large subunit, a locus used for phylogenetic studies
                                                        of plants. </xsd:documentation>
                                                    <xs:appinfo>
                                                        <ann:Glossary/>
                                                        <ann:Status current="Active"/>
                                                    </xs:appinfo>
                                                </xsd:annotation>
                                            </xsd:enumeration>
                                            <xsd:enumeration value="matK">
                                                <xsd:annotation>
                                                    <xsd:documentation> Maturase K gene, a locus used for phylogenetic studies of
                                                        plants. </xsd:documentation>
                                                    <xs:appinfo>
                                                        <ann:Glossary/>
                                                        <ann:Status current="Active"/>
                                                    </xs:appinfo>
                                                </xsd:annotation>
                                            </xsd:enumeration>
                                            <xsd:enumeration value="COX1">
                                                <xsd:annotation>
                                                    <xsd:documentation> Mitochondrial cytochrome c oxidase 1 gene, a locus used for
                                                        phylogenetic studies of animals </xsd:documentation>
                                                    <xs:appinfo>
                                                        <ann:Glossary/>
                                                        <ann:Status current="Active"/>
                                                    </xs:appinfo>
                                                </xsd:annotation>
                                            </xsd:enumeration>
                                            <xsd:enumeration value="ITS1-5.8S-ITS2">
                                                <xsd:annotation>
                                                    <xsd:documentation> Internal transcribed spacers 1 and 2 plus 5.8S rRNA region,
                                                        a locus used for phylogenetic studies of fungi. </xsd:documentation>
                                                    <xs:appinfo>
                                                        <ann:Glossary/>
                                                        <ann:Status current="Active"/>
                                                    </xs:appinfo>
                                                </xsd:annotation>
                                            </xsd:enumeration>
                                            <xsd:enumeration value="exome">
                                                <xsd:annotation>
                                                    <xsd:documentation> All exonic regions of the genome. </xsd:documentation>
                                                    <xs:appinfo>
                                                        <ann:Glossary/>
                                                        <ann:Status current="Active"/>
                                                    </xs:appinfo>
                                                </xsd:annotation>
                                            </xsd:enumeration>

                                            <xsd:enumeration value="other">
                                                <xsd:annotation>
                                                    <xsd:documentation> Other locus, please describe. </xsd:documentation>
                                                    <xs:appinfo>
                                                        <ann:Glossary/>
                                                        <ann:Status current="Active"/>
                                                    </xs:appinfo>
                                                </xsd:annotation>
                                            </xsd:enumeration>
                                        </xsd:restriction>
                                    </xsd:simpleType>
                                </xsd:attribute>
                                <xsd:attribute name="description" type="xs:string">
                                    <xsd:annotation>
                                        <xsd:documentation> Submitter supplied description of alternate locus and auxiliary
                                            information. </xsd:documentation>
                                        <xs:appinfo>
                                            <ann:Glossary/>
                                            <ann:Status current="Active"/>
                                        </xs:appinfo>
                                    </xsd:annotation>
                                </xsd:attribute>
                            </xsd:complexType>
                        </xsd:element>

                    </xsd:sequence>


                </xsd:complexType>
            </xsd:element>
            <xsd:element name="POOLING_STRATEGY" minOccurs="0" maxOccurs="1">
                <xsd:annotation>
                    <xsd:documentation> The optional pooling strategy indicates how the library or libraries are organized if
                        multiple samples are involved. </xsd:documentation>
                    <xsd:appinfo>
                        <ann:Glossary/>
                        <ann:Status current="Active"/>
                    </xsd:appinfo>
                </xsd:annotation>
                <xsd:simpleType>
                    <xsd:restriction base="xs:string"> </xsd:restriction>
                </xsd:simpleType>
            </xsd:element>
            <xsd:element name="LIBRARY_CONSTRUCTION_PROTOCOL" type="xs:string" minOccurs="0" maxOccurs="1">
                <xsd:annotation>
                    <xsd:documentation> Free form text describing the protocol by which the sequencing library was constructed. </xsd:documentation>
                    <xsd:appinfo>
                        <ann:Glossary/>
                        <ann:Status current="Active"/>
                    </xsd:appinfo>
                </xsd:annotation>
            </xsd:element>
        </xsd:sequence>

    </xsd:complexType>
    <xsd:complexType name="SpotDescriptorType">
        <xsd:annotation>
            <xsd:documentation> The SPOT_DESCRIPTOR specifies how to decode the individual reads of interest from the monolithic
                spot sequence. The spot descriptor contains aspects of the experimental design, platform, and processing
                information. There will be two methods of specification: one will be an index into a table of typical decodings, the
                other being an exact specification. </xsd:documentation>
            <xsd:appinfo>
                <ann:Glossary/>
                <ann:Status current="Active"/>
            </xsd:appinfo>
        </xsd:annotation>
        <xsd:choice>
            <xsd:element name="SPOT_DECODE_METHOD" type="xsd:unsignedInt">
                <xsd:annotation>
                    <xsd:documentation> DEPRECATED. </xsd:documentation>
                    <xsd:appinfo>
                        <ann:Glossary/>
                        <ann:Status current="Deprecated"/>
                    </xsd:appinfo>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="SPOT_DECODE_SPEC">
                <xsd:complexType>
                    <xsd:sequence>
                        <xsd:element name="NUMBER_OF_READS_PER_SPOT" type="xsd:unsignedInt" minOccurs="0" maxOccurs="1">
                            <xsd:annotation>
                                <xsd:documentation> DEPRECATED. Number of tags (reads) per spot. </xsd:documentation>
                                <xsd:appinfo>
                                    <ann:Glossary/>
                                    <ann:Status current="Deprecated"/>
                                </xsd:appinfo>
                            </xsd:annotation>
                        </xsd:element>
                        <xsd:element name="SPOT_LENGTH" type="xsd:nonNegativeInteger" minOccurs="0" maxOccurs="1">
                            <xsd:annotation>
                                <xsd:documentation> Number of base/color calls, cycles, or flows per spot (raw sequence length or
                                    flow length including all application and technical tags and mate pairs, but not including gap
                                    lengths). This value will be platform dependent, library dependent, and possibly run dependent.
                                    Variable length platforms will still have a constant flow/cycle length. </xsd:documentation>
                                <xsd:appinfo>
                                    <ann:Glossary/>
                                    <ann:Status current="Active"/>
                                </xsd:appinfo>
                            </xsd:annotation>
                        </xsd:element>
                        <xsd:element name="ADAPTER_SPEC" maxOccurs="1" minOccurs="0" nillable="false" type="xs:string">
                            <xsd:annotation>
                                <xsd:documentation> Some technologies will require knowledge of the sequencing adapter or the last
                                    base of the adapter in order to decode the spot. </xsd:documentation>
                                <xsd:appinfo>
                                    <ann:Glossary/>
                                    <ann:Status current="Deprecated"/>
                                </xsd:appinfo>
                            </xsd:annotation>
                        </xsd:element>
                        <xsd:element name="READ_SPEC" minOccurs="1" maxOccurs="unbounded">
                            <xsd:complexType>
                                <xsd:sequence>
                                    <xsd:element name="READ_INDEX" type="xsd:nonNegativeInteger" nillable="false">
                                        <xsd:annotation>
                                            <xsd:documentation>READ_INDEX starts at 0 and is incrementally increased for each
                                                sequential READ_SPEC within a SPOT_DECODE_SPEC</xsd:documentation>
                                            <xsd:appinfo>
                                                <ann:Glossary/>
                                                <ann:Status current="Active"/>
                                            </xsd:appinfo>
                                        </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="READ_LABEL" type="xs:string" minOccurs="0" maxOccurs="1">
                                        <xsd:annotation>
                                            <xsd:documentation>READ_LABEL is a name for this tag, and can be used to on output to
                                                determine read name, for example F or R.</xsd:documentation>
                                            <xsd:appinfo>
                                                <ann:Glossary/>
                                                <ann:Status current="Active"/>
                                            </xsd:appinfo>
                                        </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="READ_CLASS">
                                        <xsd:simpleType>
                                            <xsd:restriction base="xs:string">
                                                <xsd:enumeration value="Application Read"/>
                                                <xsd:enumeration value="Technical Read"/>
                                            </xsd:restriction>
                                        </xsd:simpleType>
                                    </xsd:element>
                                    <xsd:element name="READ_TYPE" default="Forward">
                                        <xsd:simpleType>
                                            <xsd:restriction base="xs:string">
                                                <xsd:enumeration value="Forward"/>
                                                <xsd:enumeration value="Reverse"/>
                                                <xsd:enumeration value="Adapter"/>
                                                <xsd:enumeration value="Primer"/>
                                                <xsd:enumeration value="Linker"/>
                                                <xsd:enumeration value="BarCode"/>
                                                <xsd:enumeration value="Other"/>
                                            </xsd:restriction>
                                        </xsd:simpleType>
                                    </xsd:element>

                                    <xsd:choice>
                                        <xsd:annotation>
                                            <xsd:documentation> There are various methods to ordering the reads on the spot. </xsd:documentation>
                                            <xsd:appinfo>
                                                <ann:Glossary/>
                                                <ann:Status current="Active"/>
                                            </xsd:appinfo>
                                        </xsd:annotation>
                                        <xsd:element name="RELATIVE_ORDER">
                                            <xsd:annotation>
                                                <xsd:documentation> The read is located beginning at the offset or cycle relative to
                                                    another read. This choice is appropriate for example when specifying a read that
                                                    follows a variable length expected sequence(s). </xsd:documentation>
                                                <xsd:appinfo>
                                                    <ann:Glossary/>
                                                    <ann:Status current="Active"/>
                                                </xsd:appinfo>
                                            </xsd:annotation>
                                            <xsd:complexType>
                                                <xsd:attribute name="follows_read_index" type="xsd:nonNegativeInteger"
                                                    use="optional">
                                                    <xsd:annotation>
                                                        <xsd:documentation> Specify the read index that precedes this read. </xsd:documentation>
                                                        <xsd:appinfo>
                                                            <ann:Glossary/>
                                                            <ann:Status current="Active"/>
                                                        </xsd:appinfo>
                                                    </xsd:annotation>
                                                </xsd:attribute>
                                                <xsd:attribute name="precedes_read_index" type="xsd:nonNegativeInteger"
                                                    use="optional">
                                                    <xsd:annotation>
                                                        <xsd:documentation> Specify the read index that follows this read. </xsd:documentation>
                                                        <xsd:appinfo>
                                                            <ann:Glossary/>
                                                            <ann:Status current="Active"/>
                                                        </xsd:appinfo>
                                                    </xsd:annotation>
                                                </xsd:attribute>
                                            </xsd:complexType>
                                        </xsd:element>
                                        <xsd:element name="BASE_COORD" type="xsd:integer">
                                            <xsd:annotation>
                                                <xsd:documentation> The location of the read start in terms of base count (1 is
                                                    beginning of spot). </xsd:documentation>
                                                <xsd:appinfo>
                                                    <ann:Glossary/>
                                                    <ann:Status current="Active"/>
                                                </xsd:appinfo>
                                            </xsd:annotation>
                                        </xsd:element>
                                        <xsd:element name="CYCLE_COORD" type="xsd:integer">
                                            <xsd:annotation>
                                                <xsd:documentation> The location of the read start in terms of cycle count (1 is
                                                    beginning of spot). </xsd:documentation>
                                                <xsd:appinfo>
                                                    <ann:Glossary/>
                                                    <ann:Status current="Deprecated"/>
                                                    <ann:Alternative xpath="SPOT_DESCRIPTOR/SPOT_DECODE_SPEC/READ_SPEC/BASE_COORD"/>
                                                </xsd:appinfo>
                                            </xsd:annotation>
                                        </xsd:element>

                                        <xsd:element name="EXPECTED_BASECALL">
                                            <xsd:annotation>
                                                <xsd:documentation> An expected basecall for a current read. Read will be
                                                    zero-length if basecall is not present. Users of this facility should start
                                                    migrating to EXPECTED_BASECALL_TABLE, as this field will be phased out. </xsd:documentation>
                                                <xsd:appinfo>
                                                    <ann:Glossary/>
                                                    <ann:Status current="Deprecated"/>
                                                    <ann:Alternative xpath="SPOT_DESCRIPTOR/SPOT_DECODE_SPEC/READ_SPEC/EXPECTED_BASECALL_TABLE"/>
                                                </xsd:appinfo>
                                            </xsd:annotation>
                                            <xsd:complexType>
                                                <xsd:simpleContent>
                                                    <xsd:extension base="xs:string">
                                                        <xsd:attribute name="default_length" type="xsd:nonNegativeInteger"
                                                            use="optional">
                                                            <xsd:annotation>
                                                                <xsd:documentation> Specify whether the spot should have a default
                                                                  length for this tag if the expected base cannot be matched. </xsd:documentation>
                                                                <xsd:appinfo>
                                                                  <ann:Glossary/>
                                                                  <ann:Status current="Active"/>
                                                                </xsd:appinfo>
                                                            </xsd:annotation>
                                                        </xsd:attribute>
                                                        <xsd:attribute name="base_coord" type="xsd:nonNegativeInteger"
                                                            use="optional">
                                                            <xsd:annotation>
                                                                <xsd:documentation> Specify an optional starting point for tag (base
                                                                  offset from 1). </xsd:documentation>
                                                                <xsd:appinfo>
                                                                  <ann:Glossary/>
                                                                  <ann:Status current="Active"/>
                                                                </xsd:appinfo>
                                                            </xsd:annotation>
                                                        </xsd:attribute>
                                                    </xsd:extension>
                                                </xsd:simpleContent>
                                            </xsd:complexType>
                                        </xsd:element>
                                        <xsd:element name="EXPECTED_BASECALL_TABLE">
                                            <xsd:annotation>
                                                <xsd:documentation> A set of choices of expected basecalls for a current read. Read
                                                    will be zero-length if none is found. </xsd:documentation>
                                                <xsd:appinfo>
                                                    <ann:Glossary/>
                                                    <ann:Status current="Active"/>
                                                </xsd:appinfo>
                                            </xsd:annotation>
                                            <xsd:complexType>
                                                <xsd:sequence minOccurs="1" maxOccurs="1">
                                                    <xsd:element name="BASECALL" maxOccurs="unbounded">
                                                        <xsd:annotation>
                                                            <xsd:documentation> Element's body contains a basecall, attribute
                                                                provide description of this read meaning as well as matching rules. </xsd:documentation>
                                                            <xsd:appinfo>
                                                                <ann:Glossary/>
                                                                <ann:Status current="Active"/>
                                                            </xsd:appinfo>
                                                        </xsd:annotation>
                                                        <xsd:complexType>
                                                            <xsd:simpleContent>
                                                                <xsd:extension base="xs:string">
                                                                  <xsd:attribute name="read_group_tag" type="xs:string"
                                                                  use="optional">
                                                                  <xsd:annotation>
                                                                  <xsd:documentation> When match occurs, the read will be tagged
                                                                  with this group membership </xsd:documentation>
                                                                  <xsd:appinfo>
                                                                  <ann:Glossary/>
                                                                  <ann:Status current="Active"/>
                                                                  </xsd:appinfo>
                                                                  </xsd:annotation>
                                                                  </xsd:attribute>
                                                                  <xsd:attribute name="min_match" type="xsd:nonNegativeInteger"
                                                                  use="optional">
                                                                  <xsd:annotation>
                                                                  <xsd:documentation> Minimum number of matches to trigger
                                                                  identification. </xsd:documentation>
                                                                  <xsd:appinfo>
                                                                  <ann:Glossary/>
                                                                  <ann:Status current="Active"/>
                                                                  </xsd:appinfo>
                                                                  </xsd:annotation>
                                                                  </xsd:attribute>
                                                                  <xsd:attribute name="max_mismatch" type="xsd:nonNegativeInteger"
                                                                  use="optional">
                                                                  <xsd:annotation>
                                                                  <xsd:documentation> Maximum number of mismatches </xsd:documentation>
                                                                  <xsd:appinfo>
                                                                  <ann:Glossary/>
                                                                  <ann:Status current="Active"/>
                                                                  </xsd:appinfo>
                                                                  </xsd:annotation>
                                                                  </xsd:attribute>
                                                                  <xsd:attribute name="match_edge">
                                                                  <xsd:annotation>
                                                                  <xsd:documentation> Where the match should occur. Changes the
                                                                  rules on how min_match and max_mismatch are counted. </xsd:documentation>
                                                                  <xsd:appinfo>
                                                                  <ann:Glossary/>
                                                                  <ann:Status current="Active"/>
                                                                  </xsd:appinfo>
                                                                  </xsd:annotation>
                                                                  <xsd:simpleType>
                                                                  <xsd:restriction base="xs:string">
                                                                  <xsd:enumeration value="full">
                                                                  <xsd:annotation>
                                                                  <xsd:documentation> Only @max_mismatch influences matching process </xsd:documentation>
                                                                  <xsd:appinfo>
                                                                  <ann:Glossary/>
                                                                  <ann:Status current="Active"/>
                                                                  </xsd:appinfo>
                                                                  </xsd:annotation>
                                                                  </xsd:enumeration>
                                                                  <xsd:enumeration value="start">
                                                                  <xsd:annotation>
                                                                  <xsd:documentation> Both matches and mismatches are counted. When
                                                                  @max_mismatch is exceeded - it is not a match. When @min_match is
                                                                  reached - match is declared. </xsd:documentation>
                                                                  <xsd:appinfo>
                                                                  <ann:Glossary/>
                                                                  <ann:Status current="Active"/>
                                                                  </xsd:appinfo>
                                                                  </xsd:annotation>
                                                                  </xsd:enumeration>
                                                                  <xsd:enumeration value="end">
                                                                  <xsd:annotation>
                                                                  <xsd:documentation> Both matches and mismatches are counted. When
                                                                  @max_mismatch is exceeded - it is not a match. When @min_match is
                                                                  reached - match is declared. </xsd:documentation>
                                                                  <xsd:appinfo>
                                                                  <ann:Glossary/>
                                                                  <ann:Status current="Active"/>
                                                                  </xsd:appinfo>
                                                                  </xsd:annotation>
                                                                  </xsd:enumeration>
                                                                  </xsd:restriction>
                                                                  </xsd:simpleType>
                                                                  </xsd:attribute>
                                                                </xsd:extension>
                                                            </xsd:simpleContent>
                                                        </xsd:complexType>
                                                    </xsd:element>
                                                </xsd:sequence>
                                                <xsd:attribute name="default_length" type="xsd:nonNegativeInteger" use="optional">
                                                    <xsd:annotation>
                                                        <xsd:documentation> Specify whether the spot should have a default length
                                                            for this tag if the expected base cannot be matched. </xsd:documentation>
                                                        <xsd:appinfo>
                                                            <ann:Glossary/>
                                                            <ann:Status current="Active"/>
                                                        </xsd:appinfo>
                                                    </xsd:annotation>
                                                </xsd:attribute>
                                                <xsd:attribute name="base_coord" type="xsd:nonNegativeInteger" use="optional">
                                                    <xsd:annotation>
                                                        <xsd:documentation> Specify an optional starting point for tag (base offset
                                                            from 1). </xsd:documentation>
                                                        <xsd:appinfo>
                                                            <ann:Glossary/>
                                                            <ann:Status current="Active"/>
                                                        </xsd:appinfo>
                                                    </xsd:annotation>
                                                </xsd:attribute>
                                            </xsd:complexType>
                                        </xsd:element>
                                    </xsd:choice>


                                </xsd:sequence>
                            </xsd:complexType>
                        </xsd:element>

                    </xsd:sequence>
                </xsd:complexType>
            </xsd:element>
        </xsd:choice>
    </xsd:complexType>



    <xsd:complexType name="GapDescriptorType">
        <xsd:annotation>
            <xsd:documentation> The GAP_DESCRIPTOR specifies how the tags that comprise a spot are to be spaced against a notional
                reference sequence. </xsd:documentation>
            <xsd:appinfo>
                <ann:Glossary/>
                <ann:Status current="Active"/>
            </xsd:appinfo>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element name="GAP" minOccurs="1" maxOccurs="unbounded">
                <xsd:complexType>
                    <xsd:sequence>
                        <xsd:element name="GAP_TYPE">
                            <xsd:annotation>
                                <xsd:documentation> Specifies the gap type and parameters. </xsd:documentation>
                                <xsd:appinfo>
                                    <ann:Glossary/>
                                    <ann:Status current="Active"/>
                                </xsd:appinfo>
                            </xsd:annotation>
                            <xsd:complexType>
                                <xsd:choice>
                                    <xsd:element name="Tandem">
                                        <xsd:annotation>
                                            <xsd:documentation> Tandem gaps between ligands. </xsd:documentation>
                                            <xsd:appinfo>
                                                <ann:Glossary/>
                                                <ann:Status current="Active"/>
                                            </xsd:appinfo>
                                        </xsd:annotation>
                                        <xsd:complexType> </xsd:complexType>
                                    </xsd:element>
                                    <xsd:element name="MatePair">
                                        <xsd:annotation>
                                            <xsd:documentation> Mated tags with predicted separation and orientation. </xsd:documentation>
                                            <xsd:appinfo>
                                                <ann:Glossary/>
                                                <ann:Status current="Active"/>
                                            </xsd:appinfo>
                                        </xsd:annotation>
                                        <xsd:complexType>
                                            <xsd:attribute name="orientation">
                                                <xsd:simpleType>
                                                    <xsd:restriction base="xs:string"> </xsd:restriction>
                                                </xsd:simpleType>
                                            </xsd:attribute>
                                        </xsd:complexType>
                                    </xsd:element>
                                    <xsd:element name="PairedEnd">
                                        <xsd:annotation>
                                            <xsd:documentation> Mated tags sequenced from two ends of a physical extent of genomic
                                                material. </xsd:documentation>
                                            <xsd:appinfo>
                                                <ann:Glossary/>
                                                <ann:Status current="Active"/>
                                            </xsd:appinfo>
                                        </xsd:annotation>
                                        <xsd:complexType> </xsd:complexType>
                                    </xsd:element>
                                </xsd:choice>
                            </xsd:complexType>
                        </xsd:element>
                        <xsd:element name="GAP_SPEC">
                            <xsd:complexType>
                                <xsd:choice>
                                    <xsd:element name="interval">
                                        <xsd:complexType>
                                            <xsd:annotation>
                                                <xsd:documentation> Specify the read label at the 5' end of the gap, or NULL if it's
                                                    the first tag. </xsd:documentation>
                                                <xsd:appinfo>
                                                    <ann:Glossary/>
                                                    <ann:Status current="Active"/>
                                                </xsd:appinfo>
                                            </xsd:annotation>
                                            <xsd:attribute name="min_length" type="xsd:integer" use="required">
                                                <xsd:annotation>
                                                    <xsd:documentation> Minimum length in base pairs of the
                                                        interval.</xsd:documentation>
                                                    <xsd:appinfo>
                                                        <ann:Glossary/>
                                                        <ann:Status current="Active"/>
                                                    </xsd:appinfo>
                                                </xsd:annotation>
                                            </xsd:attribute>
                                            <xsd:attribute name="max_length" type="xsd:integer" use="required">
                                                <xsd:annotation>
                                                    <xsd:documentation> Minimum length in base pairs of the
                                                        interval.</xsd:documentation>
                                                    <xsd:appinfo>
                                                        <ann:Glossary/>
                                                        <ann:Status current="Active"/>
                                                    </xsd:appinfo>
                                                </xsd:annotation>
                                            </xsd:attribute>
                                        </xsd:complexType>
                                    </xsd:element>
                                    <xsd:element name="statistic_mean">
                                        <xsd:complexType>
                                            <xsd:annotation>
                                                <xsd:documentation> Specify the read label at the 5' end of the gap, or NULL if it's
                                                    the first tag. </xsd:documentation>
                                                <xsd:appinfo>
                                                    <ann:Glossary/>
                                                    <ann:Status current="Active"/>
                                                </xsd:appinfo>
                                            </xsd:annotation>
                                            <xsd:attribute name="mean" type="xsd:float" use="required">
                                                <xsd:annotation>
                                                    <xsd:documentation> Mean length in base pairs of the
                                                        interval.</xsd:documentation>
                                                    <xsd:appinfo>
                                                        <ann:Glossary/>
                                                        <ann:Status current="Active"/>
                                                    </xsd:appinfo>
                                                </xsd:annotation>
                                            </xsd:attribute>
                                            <xsd:attribute name="stdev" type="xsd:float" use="required">
                                                <xsd:annotation>
                                                    <xsd:documentation> Standard deviation of length in base pairs of the
                                                        interval.</xsd:documentation>
                                                    <xsd:appinfo>
                                                        <ann:Glossary/>
                                                        <ann:Status current="Active"/>
                                                    </xsd:appinfo>
                                                </xsd:annotation>
                                            </xsd:attribute>
                                        </xsd:complexType>
                                    </xsd:element>
                                    <xsd:element name="statistic_median">
                                        <xsd:complexType>
                                            <xsd:annotation>
                                                <xsd:documentation> Specify the read label at the 5' end of the gap, or NULL if it's
                                                    the first tag. </xsd:documentation>
                                                <xsd:appinfo>
                                                    <ann:Glossary/>
                                                    <ann:Status current="Active"/>
                                                </xsd:appinfo>
                                            </xsd:annotation>
                                            <xsd:attribute name="median" type="xsd:integer" use="required">
                                                <xsd:annotation>
                                                    <xsd:documentation> Median length in base pairs of the interval. Equivalent to
                                                        the BAM file @RG/PI tag.</xsd:documentation>
                                                    <xsd:appinfo>
                                                        <ann:Glossary/>
                                                        <ann:Status current="Active"/>
                                                    </xsd:appinfo>
                                                </xsd:annotation>
                                            </xsd:attribute>
                                        </xsd:complexType>
                                    </xsd:element>
                                    <xsd:element name="histogram">
                                        <xsd:complexType>
                                            <xsd:annotation>
                                                <xsd:documentation> Frequency distribution of gap length values. </xsd:documentation>
                                                <xsd:appinfo>
                                                    <ann:Glossary/>
                                                    <ann:Status current="Active"/>
                                                </xsd:appinfo>
                                            </xsd:annotation>
                                            <xsd:sequence>
                                                <xsd:element name="bin" minOccurs="1" maxOccurs="unbounded">
                                                    <xsd:complexType>
                                                        <xsd:annotation>
                                                            <xsd:documentation> Length value bin </xsd:documentation>
                                                            <xsd:appinfo>
                                                                <ann:Glossary/>
                                                                <ann:Status current="Active"/>
                                                            </xsd:appinfo>
                                                        </xsd:annotation>
                                                        <xsd:attribute name="gap_size" type="xsd:integer" use="required">
                                                            <xsd:annotation>
                                                                <xsd:documentation> Gap size in base pairs. Gap sizes can be
                                                                  expressed as actual values, or k-iles where k is the number of
                                                                  bins. Gap sizes can be negative to reflect overlaps, or 0 to
                                                                  reflect adjacent extents (eg no gap). </xsd:documentation>
                                                                <xsd:appinfo>
                                                                  <ann:Glossary/>
                                                                  <ann:Status current="Active"/>
                                                                </xsd:appinfo>
                                                            </xsd:annotation>
                                                        </xsd:attribute>
                                                        <xsd:attribute name="gap_count" type="xsd:double" use="required">
                                                            <xsd:annotation>
                                                                <xsd:documentation> Frequency count or 0. Abolute counts are
                                                                  integers greater than 0, relative counts in the range
                                                                  [0,1].</xsd:documentation>
                                                                <xsd:appinfo>
                                                                  <ann:Glossary/>
                                                                  <ann:Status current="Active"/>
                                                                </xsd:appinfo>
                                                            </xsd:annotation>
                                                        </xsd:attribute>
                                                        <xsd:attribute name="count_type" use="required">
                                                            <xsd:annotation>
                                                                <xsd:documentation> Whether absolute or relative frequency. </xsd:documentation>
                                                                <xsd:appinfo>
                                                                  <ann:Glossary/>
                                                                  <ann:Status current="Active"/>
                                                                </xsd:appinfo>
                                                            </xsd:annotation>
                                                            <xsd:simpleType>
                                                                <xsd:restriction base="xs:string">
                                                                  <xsd:enumeration value="absolute_frequency">
                                                                  <xsd:annotation>
                                                                  <xsd:documentation> Absolute raw count of hits in this bin. </xsd:documentation>
                                                                  <xsd:appinfo>
                                                                  <ann:Glossary/>
                                                                  <ann:Status current="Active"/>
                                                                  </xsd:appinfo>
                                                                  </xsd:annotation>
                                                                  </xsd:enumeration>
                                                                  <xsd:enumeration value="relative_frequency">
                                                                  <xsd:annotation>
                                                                  <xsd:documentation> Relative count of hits in this bin. All counts
                                                                  should add to 1.</xsd:documentation>
                                                                  <xsd:appinfo>
                                                                  <ann:Glossary/>
                                                                  <ann:Status current="Active"/>
                                                                  </xsd:appinfo>
                                                                  </xsd:annotation>
                                                                  </xsd:enumeration>
                                                                </xsd:restriction>
                                                            </xsd:simpleType>
                                                        </xsd:attribute>
                                                    </xsd:complexType>
                                                </xsd:element>
                                            </xsd:sequence>
                                        </xsd:complexType>
                                    </xsd:element>
                                </xsd:choice>
                                <xsd:attribute name="link5" use="optional" default="NULL">
                                    <xsd:annotation>
                                        <xsd:documentation> Specify the read label at the 5' end of the gap, or NULL if it's the
                                            first tag. </xsd:documentation>
                                        <xsd:appinfo>
                                            <ann:Glossary/>
                                            <ann:Status current="Active"/>
                                        </xsd:appinfo>
                                    </xsd:annotation>
                                </xsd:attribute>
                                <xsd:attribute name="link3" use="optional" default="NULL">
                                    <xsd:annotation>
                                        <xsd:documentation> Specify the read label at the 3' end of the gap, or NULL if it's the
                                            last tag. </xsd:documentation>
                                        <xsd:appinfo>
                                            <ann:Glossary/>
                                            <ann:Status current="Active"/>
                                        </xsd:appinfo>
                                    </xsd:annotation>
                                </xsd:attribute>
                            </xsd:complexType>
                        </xsd:element>
                    </xsd:sequence>
                </xsd:complexType>
            </xsd:element>
        </xsd:sequence>
    </xsd:complexType>

    <xsd:complexType name="LibraryType">
        <xsd:sequence>
            <xsd:element name="DESIGN_DESCRIPTION" type="xs:string">
                <xsd:annotation>
                    <xsd:documentation>Goal and setup of the individual library.</xsd:documentation>
                    <xsd:appinfo>
                        <ann:Status current="Active"/>
                    </xsd:appinfo>
                </xsd:annotation>
            </xsd:element>

            <xsd:element name="SAMPLE_DESCRIPTOR" type="com:SampleDescriptorType">
                <xsd:annotation>
                    <xsd:documentation> Pick a sample to associate this experiment with. The sample may be an individual or a pool,
                        depending on how it is specified. </xsd:documentation>
                    <xsd:appinfo>
                        <ann:Status current="Active"/>
                    </xsd:appinfo>
                </xsd:annotation>
            </xsd:element>

            <xsd:element name="LIBRARY_DESCRIPTOR" type="com:LibraryDescriptorType">
                <xsd:annotation>
                    <xsd:documentation> The LIBRARY_DESCRIPTOR specifies the origin of the material being sequenced and any
                        treatments that the material might have undergone that affect the sequencing result. This specification is
                        needed even if the platform does not require a library construction step per se. </xsd:documentation>
                    <xsd:appinfo>
                        <ann:Status current="Active"/>
                    </xsd:appinfo>
                </xsd:annotation>
            </xsd:element>

            <xsd:element name="SPOT_DESCRIPTOR" type="com:SpotDescriptorType" minOccurs="0" maxOccurs="1">
                <xsd:annotation>
                    <xsd:documentation> The SPOT_DESCRIPTOR specifies how to decode the individual reads of interest from the
                        monolithic spot sequence. The spot descriptor contains aspects of the experimental design, platform, and
                        processing information. There will be two methods of specification: one will be an index into a table of
                        typical decodings, the other being an exact specification. This construct is needed for loading data and for
                        interpreting the loaded runs. It can be omitted if the loader can infer read layout (from multiple input
                        files or from one input files). </xsd:documentation>
                    <xsd:appinfo>
                        <ann:Status current="Active"/>
                    </xsd:appinfo>
                </xsd:annotation>
            </xsd:element>

            <xsd:element name="GAP_DESCRIPTOR" type="com:GapDescriptorType" minOccurs="0" maxOccurs="1">
                <xsd:annotation>
                    <xsd:documentation> The GAP_DESCRIPTOR specifies how to place the individual tags in the spot against a notinoal
                        reference sequence. This information is important to interpreting the placement of spot tags in an assembly
                        or alignment for the purpose of detecting structural variations and other genomic features. </xsd:documentation>
                    <xsd:appinfo>
                        <ann:Status current="Active"/>
                    </xsd:appinfo>
                </xsd:annotation>
            </xsd:element>
        </xsd:sequence>

    </xsd:complexType>

    <xsd:complexType name="PlatformType">
        <xsd:annotation>
            <xsd:documentation> The PLATFORM record selects which sequencing platform and platform-specific runtime parameters. This
                will be determined by the Center. </xsd:documentation>
            <xsd:appinfo>
                <ann:Glossary/>
                <ann:Status current="Active"/>
            </xsd:appinfo>
        </xsd:annotation>
        <xsd:choice>
            <xsd:element name="LS454">
                <xsd:annotation>
                    <xsd:documentation> 454 technology use 1-color sequential flows </xsd:documentation>
