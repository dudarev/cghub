                                    <xsd:enumeration value="AB SOLiD System 3.0">
                                        <xsd:annotation>
                                            <xsd:documentation>AB SOLiD System 3.0</xsd:documentation>
                                            <xsd:appinfo>
                                                <ann:Glossary/>
                                                <ann:Status current="Active"/>
                                            </xsd:appinfo>
                                        </xsd:annotation>
                                    </xsd:enumeration>
                                    <xsd:enumeration value="AB SOLiD 3 Plus System">
                                        <xsd:annotation>
                                            <xsd:documentation>SOLiD 3 Plus System</xsd:documentation>
                                            <xsd:appinfo>
                                                <ann:Glossary/>
                                                <ann:Status current="Active"/>
                                            </xsd:appinfo>
                                        </xsd:annotation>
                                    </xsd:enumeration>
                                    <xsd:enumeration value="AB SOLiD 4 System">
                                        <xsd:annotation>
                                            <xsd:documentation>AB SOLiD System 4.x</xsd:documentation>
                                            <xsd:appinfo>
                                                <ann:Glossary/>
                                                <ann:Status current="Active"/>
                                            </xsd:appinfo>
                                        </xsd:annotation>
                                    </xsd:enumeration>
                                    <xsd:enumeration value="AB SOLiD 4hq System">
                                        <xsd:annotation>
                                            <xsd:documentation>AB SOLiD System 4hq</xsd:documentation>
                                            <xsd:appinfo>
                                                <ann:Glossary/>
                                                <ann:Status current="Active"/>
                                            </xsd:appinfo>
                                        </xsd:annotation>
                                    </xsd:enumeration>
                                    <xsd:enumeration value="AB SOLiD PI System">
                                        <xsd:annotation>
                                            <xsd:documentation>AB SOLiD System PI</xsd:documentation>
                                            <xsd:appinfo>
                                                <ann:Glossary/>
                                                <ann:Status current="Active"/>
                                            </xsd:appinfo>
                                        </xsd:annotation>
                                    </xsd:enumeration>
                                    <xsd:enumeration value="AB SOLiD 5500">
                                        <xsd:annotation>
                                            <xsd:documentation>AB SOLiD System 5500</xsd:documentation>
